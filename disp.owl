Rule: 
    disp:OilFlow(?e), disp:OilWell(?w), gufo:participatedIn(?w, ?e), disp:hasFlowVelocity(?e, ?v), gufo:inheresIn(?qv, ?w), disp:CriticalFlowVelocity(?qv), gufo:hasQualityValue(?qv, ?maxv), greaterThanOrEqual(?v, ?maxv) -> disp:OilFlowAtSupercriticalVelocity(?e)

Rule: 
    disp:OilFlow(?e), disp:OilWell(?w), gufo:participatedIn(?w, ?e), disp:hasFlowVelocity(?e, ?v), gufo:inheresIn(?qv, ?w), disp:CriticalFlowVelocity(?qv), gufo:hasQualityValue(?qv, ?maxv), lessThan(?v, ?maxv) -> disp:OilFlowAtSubcriticalVelocity(?e)

Rule: 
    disp:Position(?p), disp:Permeability(?dp), disp:Viscosity(?dv), disp:FluxPotential(?df), gufo:manifestedIn(?dp, ?e), gufo:manifestedIn(?dv, ?e), gufo:manifestedIn(?df, ?e), gufo:contributedToTrigger(?pre, ?e), gufo:standsIn(?p, ?pre), gufo:concernsQualityValue(?pre, ?p0), gufo:broughtAbout(?e, ?post), disp:hasImpetus(?dp, ?ip), disp:hasImpetus(?dv, ?iv), disp:hasImpetus(?df, ?if), divide(?x, ?ip, ?iv), multiply(?v, ?x, ?if), disp:hasDurationInSeconds(?e, ?t), multiply(?delta, ?t, ?v), add(?p1, ?delta, ?p0) -> gufo:standsIn(?p, ?post), gufo:concernsQualityValue(?post, ?p1), disp:hasFlowVelocity(?e, ?v)

Rule: 
    disp:influences(?d, ?q), gufo:inheresIn(?d, ?a), gufo:inheresIn(?q, ?p),  DifferentFrom (?a, ?p) -> disp:AgentDisposition(?d)

Rule: 
    disp:influences(?d, ?q), gufo:inheresIn(?d, ?b), gufo:inheresIn(?q, ?b) -> disp:PatientDisposition(?d)

Rule: 
    disp:influences(?d, ?q1), disp:influences(?d, ?q2),  DifferentFrom (?q1, ?q2) -> disp:ComplexDisposition(?d)

